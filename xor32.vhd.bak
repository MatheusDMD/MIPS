library IEEE;
use IEEE.STD_LOGIC_1164.ALL;

entity xor32 is
    Port ( A1 : in  STD_LOGIC_VECTOR(31 downto 0);      -- XOR gate input 1
           A2 : in  STD_LOGIC_VECTOR(31 downto 0);      -- XOR gate input 2
           X1 : out  STD_LOGIC:= '0';     -- XOR gate output
			  );
end xor32;

architecture Behavioral of xor32 is
begin
	process(A1,A2)
		begin
			if((A1 xor A2) = others=> '0') then
				X1 <= '1';		-- 2 input exclusive-OR
			else
				X1 <= '0';
			end if;
	end process;
end Behavioral;